{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "üèéÔ∏è InfraWhisperer ‚Äî Agent Activity Overview",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "title": "MCP Tool Calls (Total)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum(mcp_tool_calls_total)",
          "legendFormat": "Total Tool Calls"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 500 }
            ]
          }
        }
      }
    },
    {
      "title": "LLM Cost (USD)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "targets": [
        {
          "expr": "sum(llm_cost_total)",
          "legendFormat": "Total Cost"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 10 }
            ]
          }
        }
      }
    },
    {
      "title": "Blocked Tool Calls (Dual LLM)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "sum(llm_blocked_tools_total)",
          "legendFormat": "Blocked"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "title": "LLM Tokens (Total)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "targets": [
        {
          "expr": "sum(llm_tokens_total)",
          "legendFormat": "Total Tokens"
        }
      ]
    },
    {
      "title": "MCP Tool Calls by Server",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 5 },
      "targets": [
        {
          "expr": "sum by (mcp_server_name) (rate(mcp_tool_calls_total[5m]))",
          "legendFormat": "{{mcp_server_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "pointSize": 5
          }
        }
      }
    },
    {
      "title": "MCP Tool Call Duration (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 5 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, tool_name) (rate(mcp_tool_call_duration_seconds_bucket[5m])))",
          "legendFormat": "{{tool_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "title": "üß† LLM Metrics",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "collapsed": false
    },
    {
      "title": "LLM Cost by Agent",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 14 },
      "targets": [
        {
          "expr": "sum by (profile_name) (llm_cost_total)",
          "legendFormat": "{{profile_name}}"
        }
      ]
    },
    {
      "title": "LLM Request Duration",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 14 },
      "targets": [
        {
          "expr": "rate(llm_request_duration_seconds_sum[5m]) / rate(llm_request_duration_seconds_count[5m])",
          "legendFormat": "{{profile_name}} avg"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "title": "Tokens Per Second (Throughput)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 14 },
      "targets": [
        {
          "expr": "llm_tokens_per_second",
          "legendFormat": "{{profile_name}} - {{model}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "title": "üõ°Ô∏è Security & Errors",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "collapsed": false
    },
    {
      "title": "MCP Tool Errors",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 23 },
      "targets": [
        {
          "expr": "sum by (tool_name) (rate(mcp_tool_calls_total{status=\"error\"}[5m]))",
          "legendFormat": "{{tool_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 60
          }
        }
      }
    },
    {
      "title": "Blocked Tools Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 23 },
      "targets": [
        {
          "expr": "sum by (profile_name) (rate(llm_blocked_tools_total[5m]))",
          "legendFormat": "{{profile_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 60,
            "lineWidth": 0
          },
          "color": { "mode": "palette-classic" }
        }
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["infrawhisperer", "mcp", "archestra"],
